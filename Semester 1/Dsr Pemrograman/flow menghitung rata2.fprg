                <assign variable="total" expression="jumlah + total"/>
            </for>
            <output expression="&quot;jumalah angka = &quot;&amp;jumlah" newline="True"/>
            <output expression="&quot;total angka = &quot;&amp;total" newline="True"/>
            <assign variable="rata2" expression="jumlah / total"/>
            <output expression="&quot;rata rata angka = &quot;&amp;rata2" newline="True"/>
        </body>
    </function>
