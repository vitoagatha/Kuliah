    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, angka, jumlah, total" type="Integer" array="False" size=""/>
            <declare name="rata2" type="Real" array="False" size=""/>
            <assign variable="jumlah" expression="0"/>
